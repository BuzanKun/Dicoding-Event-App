<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.dicoding.dicodingeventapp.ui.fragments.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.dicoding.dicodingeventapp.ui.fragments.FinishedFragment"
        android:label="Finished"
        tools:layout="@layout/fragment_finished" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.dicoding.dicodingeventapp.ui.fragments.UpcomingFragment"
        android:label="Upcoming"
        tools:layout="@layout/fragment_upcoming" />

</navigation>